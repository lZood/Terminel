# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# External APIs
OPENWEATHER_API_KEY=your_openweather_api_key
CONAGUA_API_KEY=your_conagua_api_key_if_available
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key

# Internal Systems (to be provided by client)
ACCOUNTING_API_URL=your_accounting_software_api_url
ACCOUNTING_API_KEY=your_accounting_api_key
SCALE_API_URL=your_scale_system_api_url
SCALE_API_KEY=your_scale_api_key

# Email Service (for notifications)
RESEND_API_KEY=your_resend_api_key
EMAIL_FROM=noreply@grupoterminel.com

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://grupoterminel.com
